{
  "apps": [
    {
      "name": "poppop-backend",
      "script": "./app.js",
      "watch": true,
      "env": {
        "NODE_ENV": "development"
      },
      "env_production": {
        "NODE_ENV": "production"
      },
      "ignore_watch": ["node_modules", "public", "logs", "logHelper", "views"],
      "exec_interpreter": "babel-node",
      "exec_mode": "fork"
    }
  ]
}
